{
    "version": "0.1.0",
    "command": "gulp",
    "isShellCommand": true,
    "tasks": [
        {
			"taskName": "build",
			// Make this the default build command.
			"isBuildCommand": true,
			// Show the output window only if unrecognized errors occur.
			"showOutput": "always",
			"problemMatcher": ["$gulp-tsc", "$tsc"]
		}
    ]
}